<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.tasafe.tasafe">


    <application
        android:allowBackup="false"
        android:icon="@drawable/logo"
        android:installLocation="internalOnly"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="br.com.tasafe.ui.activity.NewSiteActivity"></activity>
        <activity android:name="br.com.tasafe.ui.activity.BaseActivity" />
        <activity android:name="br.com.tasafe.ui.activity.MySitesActivity" />
        <activity
            android:name="br.com.tasafe.ui.activity.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>